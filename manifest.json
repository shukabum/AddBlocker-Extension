{
  "name": "SS Adblocker",
  "version": "2.0",
  "description": "Personal Ad blocker",
  "permissions": ["webRequest", "webRequestBlocking", "https://*/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
